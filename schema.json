{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
        "pages",
        "anchors",
        "objects"
    ],
    "properties": {
        "pages": {
            "$id": "#/properties/pages",
            "type": "array",
            "title": "The pages Schema",
            "items": {
                "$id": "#/properties/pages/objects",
                "type": "object",
                "title": "The Anchor Objects Schema",
                "required": [
                    "regex_id",
                    "obj_regex",
                    "groups"
                ],
                "properties": {
                    "regex_id": {
                        "$id": "#/properties/pages/items/properties/regex_id",
                        "type": "string",
                        "title": "The regex_id Schema",
                        "pattern": "^(.*)$"
                    },
                    "obj_regex": {
                        "$id": "#/properties/pages/items/properties/obj_regex",
                        "type": "string",
                        "title": "The obj_regex Schema",
                        "pattern": "^(.*)$"
                    },
                    "groups": {
                        "$id": "#/properties/pages/items/properties/groups",
                        "type": "array",
                        "title": "The groups Schema",
                        "items": {
                            "$id": "#/properties/pages/items/properties/groups/items",
                            "type": "integer",
                            "title": "The Items Schema"
                        }
                    }
                }
            }
        },
        "anchors": {
            "$id": "#/properties/anchors",
            "type": "array",
            "title": "The anchors Schema",
            "items": {
                "$id": "#/properties/anchors/objects",
                "type": "object",
                "title": "The Anchor Objects Schema",
                "required": [
                    "content_id",
                    "content_",
                    "content_alt",
                    "save_anchor",
                    "sub_groups"
                ],
                "properties": {
                    "content_id": {
                        "$id": "#/properties/sprints/items/properties/content_id",
                        "type": "string",
                        "title": "The Content_id Schema",
                        "pattern": "^(.*)$"
                    },
                    "content_": {
                        "$id": "#/properties/sprints/items/properties/content_",
                        "type": "string",
                        "title": "The Content_ Schema",
                        "pattern": "^(.*)$"
                    },
                    "content_alt": {
                        "$id": "#/properties/sprints/items/properties/content_alt",
                        "type": "string",
                        "title": "The Content_alt Schema",
                        "pattern": "^(.*)$"
                    },
                    "save_anchor": {
                        "$id": "#/properties/sprints/items/properties/save_anchor",
                        "type": "boolean",
                        "title": "The Save_anchor Schema"
                    },
                    "sub_groups": {
                        "$id": "#/properties/sprints/items/properties/sub_groups",
                        "type": "array",
                        "title": "The Sub_groups Schema",
                        "items": {
                            "$id": "#/properties/sprints/items/properties/sub_groups/items",
                            "type": "integer",
                            "title": "The Items Schema"
                        }
                    }
                }
            }
        },
        "objects": {
            "$id": "#/properties/objects",
            "type": "array",
            "title": "The Objects Schema",
            "items": {
                "$id": "#/properties/objects/items",
                "type": "object",
                "title": "The Items Schema",
                "required": [
                    "id",
                    "text_justify",
                    "maximum_values",
                    "y_object_spacing",
                    "round_cut_off",
                    "is_int",
                    "minimum_value",
                    "calc_modes",
                    "avrg_source_group",
                    "avrg_source_sub_group",
                    "avrg_base_group",
                    "avrg_base_sub_group",
                    "avrg_multiplier",
                    "overflow_threshold",
                    "sort_settings",
                    "re_comp_name",
                    "re_group_count",
                    "regexs",
                    "margin_x",
                    "margin_y",
                    "object_count"
                ],
                "properties": {
                    "id": {
                        "$id": "#/properties/objects/objects/properties/id",
                        "type": "integer",
                        "title": "The id Schema"
                    },
                    "text_justify": {
                        "$id": "#/properties/objects/objects/properties/text_justify",
                        "type": "string",
                        "title": "The text_justify Schema",
                        "pattern": "^(.*)$"
                    },
                    "maximum_values": {
                        "$id": "#/properties/objects/objects/properties/maximum_values",
                        "type": "integer",
                        "title": "The maximum_values Schema"
                    },
                    "y_object_spacing": {
                        "$id": "#/properties/objects/objects/properties/y_object_spacing",
                        "type": "number",
                        "title": "The y_object_spacing Schema"
                    },
                    "round_cut_off": {
                        "$id": "#/properties/objects/objects/properties/round_cut_off",
                        "type": "number",
                        "title": "The round_cut_off Schema"
                    },
                    "is_int": {
                        "$id": "#/properties/objects/objects/properties/is_int",
                        "type": "boolean",
                        "title": "The is_int Schema"
                    },
                    "minimum_value": {
                        "$id": "#/properties/objects/objects/properties/minimum_value",
                        "type": "number",
                        "title": "The minimum_value Schema"
                    },
                    "calc_modes": {
                        "$id": "#/properties/objects/objects/properties/calc_modes",
                        "type": "array",
                        "title": "The calc_modes Schema",
                        "items": {
                            "$id": "#/properties/objects/objects/properties/calc_modes/items",
                            "type": "string",
                            "title": "The Items Schema"
                        }
                    },
                    "avrg_self": {
                        "$id": "#/properties/objects/objects/properties/avrg_self",
                        "type": "boolean",
                        "title": "The avrg_self Schema"
                    },
                    "avrg_source_group": {
                        "$id": "#/properties/objects/objects/properties/avrg_source_group",
                        "type": "string",
                        "title": "The avrg_source_group Schema",
                        "pattern": "^(.*)$"
                    },
                    "avrg_source_sub_group": {
                        "$id": "#/properties/objects/objects/properties/avrg_source_sub_group",
                        "type": "string",
                        "title": "The avrg_source_sub_group Schema",
                        "pattern": "^(.*)$"
                    },
                    "avrg_base_group": {
                        "$id": "#/properties/objects/objects/properties/avrg_base_group",
                        "type": "string",
                        "title": "The avrg_base_group Schema",
                        "pattern": "^(.*)$"
                    },
                    "avrg_base_sub_group": {
                        "$id": "#/properties/objects/objects/properties/avrg_base_sub_group",
                        "type": "string",
                        "title": "The avrg_base_sub_group Schema",
                        "pattern": "^(.*)$"
                    },
                    "avrg_multiplier": {
                        "$id": "#/properties/objects/objects/properties/avrg_multiplier",
                        "type": "integer",
                        "title": "The avrg_multiplier Schema"
                    },
                    "overflow_threshold": {
                        "$id": "#/properties/objects/objects/properties/overflow_threshold",
                        "type": "number",
                        "title": "The overflow_threshold Schema"
                    },
                    "sort_settings": {
                        "$id": "#/properties/objects/objects/properties/sort_settings",
                        "type": "array",
                        "title": "The sort_settings Schema",
                        "items": {
                            "$id": "#/properties/objects/objects/properties/sort_settings/items",
                            "type": "boolean",
                            "title": "The Items Schema"
                        }
                    },
                    "re_comp_name": {
                        "$id": "#/properties/objects/objects/properties/re_comp_name",
                        "type": "string",
                        "title": "The re_comp_name Schema",
                        "pattern": "^(.*)$"
                    },
                    "re_group_count": {
                        "$id": "#/properties/objects/objects/properties/re_group_count",
                        "type": "integer",
                        "title": "The re_group_count Schema"
                    },
                    "regexs": {
                        "$id": "#/properties/objects/objects/properties/regexs",
                        "type": "array",
                        "title": "The regexs Schema",
                        "items": {
                            "$id": "#/properties/objects/objects/properties/regexs/items",
                            "type": "string",
                            "title": "The Items Schema"
                        }
                    },
                    "margin_x": {
                        "$id": "#/properties/objects/objects/properties/margin_x",
                        "type": "number",
                        "title": "The margin_x Schema"
                    },
                    "margin_y": {
                        "$id": "#/properties/objects/objects/properties/margin_y",
                        "type": "number",
                        "title": "The margin_y Schema"
                    },
                    "object_count": {
                        "$id": "#/properties/objects/objects/properties/object_count",
                        "type": "integer",
                        "title": "The object_count Schema"
                    }
                }
            }
        }
    }
}